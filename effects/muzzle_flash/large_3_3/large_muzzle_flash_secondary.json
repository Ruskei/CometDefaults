{
	"macros": {
		"_size$factor_": {
			"type": "catmull",
			"input": "particle.age",
			"x": [0.0, 2.0],
			"y": [1.0, 0.5]
		},
		"_size_": "particle_variable.random2 * 3.0 + 3.0"
	},
	"components": {
		"default_environment_data": {
			"start_color": "#feedac",
			"end_color": "#fc965e"
		},
		"display_type": "fixed",
		"randoms": {
			"particle": 4
		},
		"emitter_rate_instant": {
			"amount": 12
		},
		"constant_sprite": {
			"sprite": "rhombus"
		},
		"expression_scale": {
			"x": "_size_ * _size$factor_ + 1.0",
			"y": "_size_ * _size$factor_ + 1.0",
			"z": 1.0
		},
		"expression_rotation": {
			"x": "Math.PI * 2.0 * particle_variable.random",
			"z": "Math.PI * (particle_variable.random3 * 0.2)",
			"y": "Math.PI * (0.5 - particle_variable.random4)"
		},
		"particle_lifetime_expression": {
			"max_lifetime": 2
		},
		"timed_emitter_lifetime": {
			"max_lifetime": 5
		},
		"gradient_color": {
			"interpolant": "_size_",
			"data": [
				{
					"index": 3.0,
					"color": "emitter_variable.start_color"
				},
				{
					"index": 6.0,
					"color": "emitter_variable.end_color"
				}
			]
		}
	}
}
